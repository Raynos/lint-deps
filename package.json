{
  "name": "lint-deps",
  "description": "Check for dependencies that aren't listed in package.json and asks if you want to add them. Also tells you if packages are listed in package.json but aren't used anywhere. Node.js command line tool and API.",
  "version": "0.3.0",
  "main": "index.js",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/lint-deps.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/lint-deps/blob/master/LICENSE-MIT"
    }
  ],
  "preferGlobal": true,
  "bin": {
    "deps": "./index.js"
  },
  "dependencies": {
    "files-filters": "^0.1.1",
    "filter-files": "^0.2.4",
    "lodash": "^2.4.1",
    "match-requires": "^0.1.1",
    "multimatch": "^1.0.0"
  },
  "keywords": [
    "dep",
    "dependencies",
    "dependency",
    "deps",
    "installed",
    "missing",
    "require",
    "required"
  ]
}
