{
  "name": "lint-deps",
  "description": "Node.js command line tool, checks for dependencies that aren't listed in package.json and asks if you want to add them. \n\nAlso tells you if packages are listed in package.json but aren't used anywhere.",
  "version": "0.2.4",
  "main": "index.js",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/lint-deps.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/lint-deps/blob/master/LICENSE-MIT"
    }
  ],
  "preferGlobal": true,
  "bin": {
    "deps": "./bin/deps.js"
  },
  "dependencies": {
    "cwd": "^0.3.3",
    "fs-utils": "^0.4.3",
    "inquirer": "^0.5.0",
    "lodash": "^2.4.1",
    "matchdep": "^0.3.0",
    "matched": "^0.1.1",
    "minimist": "^0.1.0",
    "plasma": "^0.2.7",
    "strip-comments": "^0.1.6",
    "verbalize": "^0.1.2",
    "word-wrap": "^0.1.2"
  },
  "keywords": [
    "deps",
    "installed",
    "dependencies",
    "require",
    "required"
  ],
  "devDependencies": {
    "verb": "~0.2.2"
  }
}
