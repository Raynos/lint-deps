{
  "name": "lint-deps",
  "description": "Command-line tool to check for dependencies that are not listed in package.json, and optionally add them. Also tells you when packages that aren't used anywhere are listed in package.json.",
  "version": "0.1.4",
  "main": "index.js",
  "homepage": "https://github.com/jonschlinkert/lint-deps",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/lint-deps.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/lint-deps/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/lint-deps/blob/master/LICENSE-MIT"
    }
  ],
  "bin": {
    "deps": "./bin/deps"
  },
  "dependencies": {
    "chalk": "~0.4.0",
    "cmdify": "0.0.4",
    "fs-utils": "~0.3.6",
    "inquirer": "~0.4.0",
    "lodash": "~2.4.1",
    "strip-comments": "~0.1.6",
    "config-file": "~0.1.4"
  },
  "keywords": [
    "deps",
    "installed",
    "dependencies",
    "require",
    "required",
    "used"
  ]
}
